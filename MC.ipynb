   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.DataFrame(\n",
    "    {\n",
    "        \"individual_id\": individual_id,\n",
    "        \"time_point_id\": time_point_id,\n",
    "        \"reward\": MC,        \n",
    "    }\n",
    ")\n",
    "df.to_csv(f\"/Users/zeyubian/Desktop/Sims/MC.csv\", index=None)"
   ]
